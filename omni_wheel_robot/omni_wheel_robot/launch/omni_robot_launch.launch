<?xml version="1.0" ?>
<launch>
	<arg name="omni_robot" />
	<!-- <param name="robot_description" textfile="$(find omni_wheel_robot)/robots/rs_robot.urdf" /> -->
	<param name="robot_description" textfile="$(find omni_wheel_robot)/urdf/robot_base.urdf" />

	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="use_gui" value="true"/>
  </node>

	<!-- <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" /> -->

	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" /> 
	
	<node name="holonomic_controller" pkg="omni_wheel_robot" type="teleopkey_node.py" output="screen" />
	
	<rosparam command="load" file="$(find omni_wheel_robot)/config/controller.yaml" />
	
	<node pkg="tf" type="static_transform_publisher" name="base_link_to_laser" args="0.275 0 0 0 0 0 base_link laser 1000" />
	
	<node pkg="tf" type="static_transform_publisher" name="map_to_odom" args="0 0 0 0 0 0 map odom 1000" />
	<!-- <node name="map_server" pkg="map_server" type="map_server" args="$(find navigation_kct)/maps/arena-map.yaml"/> -->
	<!-- <node name="pose_to_path" pkg="omni_wheel_robot" type="wheel_odom_sanjay.py" output="screen" /> -->
	<!-- <node pkg="tf" type="static_transform_publisher" name="odom_to_base_link" args="0 0 0 0 0 0 odom base_link 1000" /> -->
	<!-- <node pkg="tf" type="static_transform_publisher" name="base_link_to_camera" args="0 0 0 0 0 0 base_link camera 1000" /> -->
	
</launch>

